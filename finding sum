// Creating programme for finding sum of the input numbers.

#include <iostream>
using namespace std;

int main() {
    int num1;
    int num2;

    // Input of first number
    cout<<"Enter first number : ";
    cin>>num1;

    // Input of second number
    cout<<"Enter second number : ";
    cin>>num2;

    // calculating sum
    int sum = num1 + num2;

    cout<<"The sum of first and second number is "<<sum<<endl;
    return 0;
}
